{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://localhost/v1/fdr_history_schema.json",
  "title": "FDR",
  "description": "Convert body",
  "type": "object",
  "required": ["payload"],
  "properties": {
    "payload": {
      "description": "Base 64 encoded payload",
      "type": "string",
      "example": "YWJjZGVmZw=="
    },
    "encoding": {
      "description": "Encoding of the payload",
      "type": "string",
      "example": "base64"
      }
  }
}