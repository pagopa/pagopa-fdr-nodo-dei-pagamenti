{
  "openapi": "3.0.3",
  "info": {
    "title": "FDR - Flussi di rendicontazione FASE 1",
    "description": "FDR - Flussi di rendicontazione FASE 1",
    "termsOfService": "https://www.pagopa.gov.it/",
    "version": "2.1.8"
  },
  "servers": [
    {
      "url": "http://localhost:8080/"
    }
  ],
  "security": [
    {
      "api_key": []
    }
  ],
  "tags": [
    {
      "name": "Fdr fase 1",
      "description": "Fdr fase 1"
    }
  ],
  "paths": {
    "/notify/fdr": {
      "post": {
        "tags": [
          "Fdr fase 1"
        ],
        "summary": "Notify fdr to convert",
        "description": "Notify fdr to convert",
        "operationId": "notifyFdrToConvert",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Fdr"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          }
        }
      }
    },
    "/nodoInviaFlussoRendicontazioneFTP": {
      "post": {
        "tags": [
          "Fdr fase 1"
        ],
        "summary": "Invio di un FDR riversato su FTP da Nexi",
        "description": "Invio di un FDR riversato su FTP da Nexi",
        "operationId": "nodoInviaFlussoRendicontazioneFTP",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NodoInviaFlussoRendicontazioneFTPReq"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "GenericResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "Success"
          }
        }
      },
      "Fdr": {
        "required": [
          "fdr",
          "pspId",
          "organizationId",
          "retry",
          "revision"
        ],
        "type": "object",
        "properties": {
          "fdr": {
            "description": "[XML NodoInviaFlussoRendicontazione]=[identificativoFlusso]",
            "pattern": "[a-zA-Z0-9\\-_]{1,35}",
            "type": "string",
            "example": "2016-08-16pspTest-1178"
          },
          "pspId": {
            "type": "string",
            "example": "1"
          },
          "organizationId": {
            "type": "string",
            "example": "1"
          },
          "retry": {
            "format": "int64",
            "type": "integer",
            "example": 1
          },
          "revision": {
            "format": "int64",
            "type": "integer",
            "example": 1
          }
        }
      },
      "NodoInviaFlussoRendicontazioneFTPReq": {
        "required": ["content"],
        "type": "object",
        "properties": {
          "content": {
            "type": "string",
            "description": "nodoInviaFlussoRendicontazione SOAP xml",
            "example": "<soap:Envelope xmlns:soap=\"http://schemas.xmlsoap.org/soap/envelope/\">\n<soap:Body>\n<ns5:nodoInviaFlussoRendicontazione>..."
          }
        }
      }
    },
    "securitySchemes": {
      "api_key": {
        "type": "apiKey",
        "name": "Ocp-Apim-Subscription-Key",
        "in": "header"
      },
      "SecurityScheme": {
        "type": "http",
        "description": "Authentication",
        "scheme": "basic"
      }
    }
  }
}
